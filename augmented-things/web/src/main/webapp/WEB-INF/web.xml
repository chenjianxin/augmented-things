<?xml version="1.0" encoding="UTF-8"?>
<web-app xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd" id="WebApp_ID" version="4.0">
  <display-name>web</display-name>
  
  <welcome-file-list>
    <welcome-file>index.html</welcome-file>
  </welcome-file-list>
  
  <servlet>
        <servlet-name>Om2mServlet</servlet-name>
        <servlet-class>mecs.iot.proj.web.Om2mServlet</servlet-class>
  </servlet>
  <servlet>
        <servlet-name>AndroidServlet</servlet-name>
        <servlet-class>mecs.iot.proj.web.AndroidServlet</servlet-class>
  </servlet>

    <!-- servlet mapping may be not required if the url pattern has
    	already been defined between @WebServlet parameters -->
</web-app>